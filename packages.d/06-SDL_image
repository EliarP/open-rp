	--prefix=$PREFIX \
	--enable-shared \
	--enable-static \
	--enable-bmp 
	--disable-gif \
	--disable-jpg \
	--disable-lbm \
	--disable-pcx \
	--enable-png \
	--disable-png-shared \
	--disable-pnm \
	--disable-tga \
	--disable-tif \
	--disable-xcf \
	--disable-xpm \
	--disable-xv"
LIBS=$PREFIX/lib/libz.a
